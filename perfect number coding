#include <stdio.h>
void func(int a)
{
    int sum=0,i;
    for(i=1;i<a;i++)
    {
        if(a%i==0)
        {
            sum=sum+i;
        }
    }
           if(sum==a) 
       printf("\nthe number is a perfect number");
    else
    
     printf("\nthe number is not a perfect number");
   
}

int main()
{
    int num;
    printf("enter a number ");
    scanf("%d",&num);
    func(num);
    return 0;
}
